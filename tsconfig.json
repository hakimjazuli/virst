{
	"compilerOptions": {
		"allowJs": true,
		"checkJs": true,
		"declaration": true,
		"emitDeclarationOnly": true,
		"outDir": "./types",
		"module": "ESNext",
		"target": "ES2020",
		"moduleResolution": "node",
		"esModuleInterop": true,
		"skipLibCheck": true,
		"noUnusedParameters": true,
		"noUnusedLocals": true
	},
	"include": ["./index.mjs"]
}
